def email_confirm(order) 

  subsite = Subsite.city
  trigger_name = 'city_order_confirm_v5'
  data_object = order
  send_to_email_address = 'michaelt@giltcity.com'
  package = order.order_items.first.unit.sku.package_option.package
  opts = {:city_site => CitySite.giltcity, :package => package}

  mta_output = Exacttarget::City::OrderConfirm.send_email(subsite, trigger_name, data_object, send_to_email_address, opts)

end

